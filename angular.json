{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-dev-kit": {
      "projectType": "library",
      "root": "projects/app-dev-kit",
      "sourceRoot": "projects/app-dev-kit/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/app-dev-kit/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/app-dev-kit/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/app-dev-kit/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/app-dev-kit/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/app-dev-kit/**/*.ts",
              "projects/app-dev-kit/**/*.html"
            ]
          }
        }
      }
    },

    "ui-components": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ui-components",
      "sourceRoot": "projects/ui-components/src",
      "prefix": "hsi-ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ui-components/src/test.ts",
            "tsConfig": "projects/ui-components/tsconfig.spec.json",
            "karmaConfig": "projects/ui-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ui-components/**/*.ts",
              "projects/ui-components/**/*.html"
            ]
          }
        }
      }
    },
    "viz-components": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/viz-components",
      "sourceRoot": "projects/viz-components/src",
      "prefix": "vic",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/viz-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/viz-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/viz-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/viz-components/src/test.ts",
            "tsConfig": "projects/viz-components/tsconfig.spec.json",
            "karmaConfig": "projects/viz-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/viz-components/**/*.ts",
              "projects/viz-components/**/*.html"
            ]
          }
        }
      }
    },
    "demo-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/demo-app",
      "sourceRoot": "projects/demo-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/demo-app",
            "index": "projects/demo-app/src/index.html",
            "browser": "projects/demo-app/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/demo-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-app/src/favicon.ico",
              "projects/demo-app/src/assets",
              {
                "glob": "**/*-content/**/*",
                "input": "projects/demo-app/src/app/content/viz-components/",
                "output": "/app/content/viz-components"
              },
              {
                "glob": "**/*-content/**/*",
                "input": "projects/demo-app/src/app/content/ui-components/",
                "output": "/app/content/ui-components"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/demo-app/src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/demo-app/src/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/demo-app/src/environments/environment.ts",
                  "with": "projects/demo-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "demo-app:build:production"
            },
            "development": {
              "buildTarget": "demo-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "demo-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo-app/src/test.ts",
            "polyfills": "projects/demo-app/src/polyfills.ts",
            "tsConfig": "projects/demo-app/tsconfig.spec.json",
            "karmaConfig": "projects/demo-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-app/src/favicon.ico",
              "projects/demo-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/demo-app/src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/demo-app/src/styles"]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo-app/**/*.ts",
              "projects/demo-app/**/*.html"
            ]
          }
        }
      }
    },
    "my-work": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/my-work",
      "sourceRoot": "projects/my-work/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/my-work",
            "index": "projects/my-work/src/index.html",
            "browser": "projects/my-work/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/my-work/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/my-work/src/favicon.ico",
              "projects/my-work/src/assets"
            ],
            "styles": ["projects/my-work/src/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/my-work/src/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "my-work:build:production"
            },
            "development": {
              "buildTarget": "my-work:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "my-work:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/my-work/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/my-work/src/favicon.ico",
              "projects/my-work/src/assets"
            ],
            "styles": ["projects/my-work/src/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/my-work/src/styles"]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/my-work/**/*.ts",
              "projects/my-work/**/*.html"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
