{
  "name": "viz-components",
  "version": "0.0.0",
  "compodoc": {
    "name": "Viz Component Library Documentation",
    "watch": true,
    "theme": "laravel",
    "disableDomTree": true,
    "disableGraph": true,
    "disableRoutesGraph": true,
    "includes": ".",
    "includesName": "more READMEs"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:demo-app": "ng serve demo-app",
    "build": "ng build",
    "build:viz-components": "ng build viz-components",
    "build:ui-components": "ng build ui-components",
    "build:demo-app": "ng build demo-app",
    "build:documentation": "npm run build:docs:ui-components && npm run build:docs:viz-components",
    "watch": "ng build --watch --configuration development",
    "watch:viz-components": "ng build viz-components --watch",
    "watch:ui-components": "ng build ui-components --watch",
    "test:viz-components": "ng test viz-components --browsers=Chrome",
    "test:viz-components:ci": "ng test viz-components --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "test:ui-components": "ng test ui-components --browsers=Chrome",
    "test:ui-components:ci": "ng test ui-components --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "lint:demo-app": "ng lint demo-app",
    "lint:viz-components": "ng lint viz-components",
    "lint:viz-components:ci": "ng lint viz-components && exit 0",
    "lint:ui-components": "ng lint ui-components",
    "compodoc:build:viz-components": "npx compodoc -p projects/viz-components/tsconfig.doc.json -d compodoc-docs/viz-components",
    "compodoc:build:ui-components": "npx compodoc -p projects/ui-components/tsconfig.doc.json -d compodoc-docs/ui-components",
    "process-docs:viz-components": "tsx projects/demo-app/documentation-generator/documentation-parser.ts --lib viz-components",
    "process-docs:ui-components": "tsx projects/demo-app/documentation-generator/documentation-parser.ts --lib ui-components",
    "build:docs:viz-components": "npm run compodoc:build:viz-components && npm run process-docs:viz-components",
    "build:docs:ui-components": "npm run compodoc:build:ui-components && npm run process-docs:ui-components",
    "cypress:open": "cypress open",
    "cypress:component:ci": "cypress run --component --browser chrome"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/router": "^17.3.11",
    "copyfiles": "^2.4.1",
    "csstype": "^3.1.3",
    "d3": "^7.8.5",
    "file-saver": "^2.0.5",
    "geo-albers-usa-territories": "^0.1.0",
    "html-to-image": "^1.10.8",
    "lodash-es": "^4.17.21",
    "marked": "^4.1.0",
    "papaparse": "^5.3.2",
    "pend": "^1.2.0",
    "polylabel": "^1.1.0",
    "prismjs": "^1.28.0",
    "rxjs": "~7.5.0",
    "serverless": "^3.21.0",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.0",
    "xmldoc": "^1.2.0",
    "yaml": "^2.1.1",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "17.5.2",
    "@angular-eslint/eslint-plugin": "17.5.2",
    "@angular-eslint/eslint-plugin-template": "17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.11",
    "@compodoc/compodoc": "^1.1.19",
    "@percy/cli": "^1.8.1",
    "@percy/cypress": "^3.1.2",
    "@schematics/angular": "^17.3.8",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.10",
    "@types/jasmine": "~4.0.0",
    "@types/topojson-client": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "cypress": "^13.6.2",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "local-cypress": "^1.2.6",
    "minimist": "^1.2.8",
    "ng-packagr": "^17.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "tsx": "^4.17.0",
    "typescript": "~5.4.5"
  }
}
