{
  "name": "frontend-shared-packages",
  "compodoc": {
    "name": "Frontend Shared Packages Documentation",
    "watch": true,
    "theme": "laravel",
    "disableDomTree": true,
    "disableGraph": true,
    "disableRoutesGraph": true,
    "includes": ".",
    "includesName": "more READMEs"
  },
  "scripts": {
    "ng": "ng",
    "setup-repo": "npm ci && husky init",
    "start": "nx serve",
    "start:demo-app": "nx serve demo-app",
    "start:my-work": "nx serve my-work",
    "build:app-dev-kit": "nx build app-dev-kit",
    "build:demo-app": "nx build demo-app",
    "build:ui-components": "nx build ui-components",
    "build:viz-components": "nx build viz-components",
    "build:documentation": "npm run build:docs:ui-components && npm run build:docs:viz-components",
    "watch": "nx build --watch --configuration development",
    "watch:app-dev-kit": "nx build app-dev-kit --watch",
    "watch:ui-components": "nx build ui-components --watch",
    "watch:viz-components": "nx build viz-components --watch",
    "test:app-dev-kit": "nx test app-dev-kit --watch=true --browsers=Chrome",
    "test:ui-components": "nx test ui-components --watch=true --browsers=Chrome",
    "test:viz-components": "nx test viz-components --watch=true --browsers=Chrome",
    "lint:app-dev-kit": "nx lint app-dev-kit",
    "lint:demo-app": "nx lint demo-app",
    "lint:my-work": "nx lint my-work",
    "lint:ui-components": "nx lint ui-components",
    "lint:viz-components": "nx lint viz-components",
    "compodoc:build:ui-components": "tsx compodoc/processing/directory-cleaner.ts --lib ui-components && npx compodoc -p libs/ui-components/tsconfig.doc.json -d compodoc/docs/ui-components",
    "compodoc:build:viz-components": "tsx compodoc/processing/directory-cleaner.ts --lib viz-components && npx compodoc -p libs/viz-components/tsconfig.doc.json -d compodoc/docs/viz-components",
    "process-docs:ui-components": "tsx compodoc/processing/documentation-processor.ts --lib ui-components",
    "process-docs:viz-components": "tsx compodoc/processing/documentation-processor.ts --lib viz-components",
    "build:docs:ui-components": "npm run compodoc:build:ui-components && npm run process-docs:ui-components",
    "build:docs:viz-components": "npm run compodoc:build:viz-components && npm run process-docs:viz-components",
    "cypress:local:ui-components": "nx run ui-components:cypress-local",
    "cypress:local:viz-components": "nx run viz-components:cypress-local",
    "cypress:component:ui-components": "nx component-test ui-components",
    "cypress:component:viz-components": "nx component-test viz-components"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.2",
    "@angular/cdk": "^19.2.6",
    "@angular/common": "19.2.2",
    "@angular/compiler": "19.2.2",
    "@angular/core": "19.2.2",
    "@angular/forms": "19.2.2",
    "@angular/material": "^19.2.6",
    "@angular/platform-browser": "19.2.2",
    "@angular/platform-browser-dynamic": "19.2.2",
    "@angular/router": "19.2.2",
    "copyfiles": "^2.4.1",
    "csstype": "^3.1.3",
    "d3": "^7.8.5",
    "file-saver": "^2.0.5",
    "geo-albers-usa-territories": "^0.1.0",
    "hast-util-from-parse5": "^8.0.1",
    "html-entities": "^2.5.2",
    "html-to-image": "^1.11.13",
    "lodash-es": "^4.17.21",
    "papaparse": "^5.3.2",
    "parse5": "^7.1.2",
    "pend": "^1.2.0",
    "polylabel": "^1.1.0",
    "prismjs": "^1.28.0",
    "rehype": "^13.0.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "rxjs": "~7.5.0",
    "serverless": "^3.21.0",
    "shiki": "^1.14.1",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.0",
    "unified": "^11.0.5",
    "xmldoc": "^1.2.0",
    "yaml": "^2.1.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.3",
    "@angular-devkit/core": "19.2.3",
    "@angular-devkit/schematics": "19.2.3",
    "@angular-eslint/builder": "19.2.0",
    "@angular-eslint/eslint-plugin": "19.2.0",
    "@angular-eslint/eslint-plugin-template": "19.2.0",
    "@angular-eslint/schematics": "19.2.0",
    "@angular-eslint/template-parser": "19.2.0",
    "@angular/cli": "~19.2.0",
    "@angular/compiler-cli": "19.2.2",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-angular": "^18.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@cypress/webpack-dev-server": "^4.0.2",
    "@jscutlery/semver": "^5.3.1",
    "@nx/angular": "^21.1.2",
    "@nx/cypress": "^21.1.2",
    "@nx/workspace": "^21.1.2",
    "@percy/cli": "^1.30.10",
    "@percy/cypress": "^3.1.6",
    "@schematics/angular": "19.2.3",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.10",
    "@types/hast": "^3.0.4",
    "@types/jasmine": "~4.0.0",
    "@types/topojson-client": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@typescript-eslint/utils": "^8.26.1",
    "cypress": "^14.4.0",
    "cypress-real-events": "^1.14.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^9.0.0",
    "jasmine-core": "^5.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "local-cypress": "^1.2.6",
    "minimist": "^1.2.8",
    "ng-packagr": "19.2.0",
    "npm-run-all": "^4.1.5",
    "nx": "^21.1.2",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "tsx": "^4.17.0",
    "typescript": "5.7.3"
  }
}
