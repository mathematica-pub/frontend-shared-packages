{
  "name": "viz-components",
  "version": "0.0.0",
  "compodoc": {
    "name": "Viz Component Library Documentation",
    "watch": true,
    "theme": "laravel",
    "disableDomTree": true,
    "disableGraph": true,
    "disableRoutesGraph": true,
    "includes": ".",
    "includesName": "more READMEs"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:demo": "ng serve demo-app",
    "build": "ng build",
    "build:lib": "ng build viz-components",
    "watch": "ng build --watch --configuration development",
    "watch:lib": "ng build viz-components --watch",
    "test": "ng test",
    "test:lib": "ng test viz-components --browsers=Chrome",
    "test:lib:ci": "ng test viz-components --no-watch --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "lint:lib": "ng lint viz-components",
    "compodoc:build": "npx compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "npx compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "npx compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@types/d3": "^7.4.0",
    "copyfiles": "^2.4.1",
    "d3": "^7.4.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular-eslint/builder": "14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin": "14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin-template": "14.0.0-alpha.3",
    "@angular-eslint/schematics": "14.0.0-alpha.3",
    "@angular-eslint/template-parser": "14.0.0-alpha.3",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.2"
  }
}