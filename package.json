{
  "name": "frontend-shared-packages",
  "compodoc": {
    "name": "Frontend Shared Packages Documentation",
    "watch": true,
    "theme": "laravel",
    "disableDomTree": true,
    "disableGraph": true,
    "disableRoutesGraph": true,
    "includes": ".",
    "includesName": "more READMEs"
  },
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "start:demo-app": "nx serve demo-app",
    "start:my-work": "nx serve my-work",
    "build:viz-components": "nx build viz-components",
    "build:ui-components": "nx build ui-components",
    "build:app-dev-kit": "nx build app-dev-kit",
    "build:demo-app": "nx build demo-app",
    "build:documentation": "npm run build:docs:ui-components && npm run build:docs:viz-components",
    "watch": "nx build --watch --configuration development",
    "watch:viz-components": "nx build viz-components --watch",
    "watch:ui-components": "nx build ui-components --watch",
    "test:viz-components": "nx test viz-components --browsers=Chrome",
    "test:viz-components:ci": "nx test viz-components --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "test:ui-components": "nx test ui-components --browsers=Chrome",
    "test:ui-components:ci": "nx test ui-components --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "lint:demo-app": "nx lint demo-app",
    "lint:my-work": "nx lint my-work",
    "lint:viz-components": "nx lint viz-components",
    "lint:ui-components": "nx lint ui-components",
    "compodoc:build:ui-components": "tsx compodoc/processing/directory-cleaner.ts --lib ui-components && npx compodoc -p projects/ui-components/tsconfig.doc.json -d compodoc/docs/ui-components",
    "compodoc:build:viz-components": "tsx compodoc/processing/directory-cleaner.ts --lib viz-components && npx compodoc -p projects/viz-components/tsconfig.doc.json -d compodoc/docs/viz-components",
    "process-docs:ui-components": "tsx compodoc/processing/documentation-processor.ts --lib ui-components",
    "process-docs:viz-components": "tsx compodoc/processing/documentation-processor.ts --lib viz-components",
    "build:docs:ui-components": "npm run compodoc:build:ui-components && npm run process-docs:ui-components",
    "build:docs:viz-components": "npm run compodoc:build:viz-components && npm run process-docs:viz-components",
    "cypress:local:viz-components": "nx run viz-components:cypress-local",
    "cypress:local:ui-components": "nx run ui-components:cypress-local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.1",
    "@angular/cdk": "^18.2.1",
    "@angular/common": "^18.2.1",
    "@angular/compiler": "^18.2.1",
    "@angular/core": "^18.2.1",
    "@angular/forms": "^18.2.1",
    "@angular/material": "^18.2.1",
    "@angular/platform-browser": "^18.2.1",
    "@angular/platform-browser-dynamic": "^18.2.1",
    "@angular/router": "^18.2.1",
    "copyfiles": "^2.4.1",
    "csstype": "^3.1.3",
    "d3": "^7.8.5",
    "file-saver": "^2.0.5",
    "geo-albers-usa-territories": "^0.1.0",
    "html-to-image": "^1.10.8",
    "lodash-es": "^4.17.21",
    "marked-gfm-heading-id": "^4.1.0",
    "marked-shiki": "^1.1.0",
    "papaparse": "^5.3.2",
    "pend": "^1.2.0",
    "polylabel": "^1.1.0",
    "prismjs": "^1.28.0",
    "rxjs": "~7.5.0",
    "serverless": "^3.21.0",
    "shiki": "^1.14.1",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.0",
    "xmldoc": "^1.2.0",
    "yaml": "^2.1.1",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular-devkit/core": "^18.2.1",
    "@angular-devkit/schematics": "^18.2.1",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "^18.2.1",
    "@compodoc/compodoc": "^1.1.19",
    "@cypress/webpack-dev-server": "^3.8.0",
    "@nx/angular": "19.7.3",
    "@nx/cypress": "^19.7.3",
    "@nx/workspace": "19.7.3",
    "@percy/cli": "^1.8.1",
    "@percy/cypress": "^3.1.2",
    "@schematics/angular": "^18.2.1",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.10",
    "@types/jasmine": "~4.0.0",
    "@types/topojson-client": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@typescript-eslint/utils": "^8.5.0",
    "cypress": "^13.13.0",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "local-cypress": "^1.2.6",
    "minimist": "^1.2.8",
    "ng-packagr": "^18.2.1",
    "npm-run-all": "^4.1.5",
    "nx": "19.7.3",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "tsx": "^4.17.0",
    "typescript": "~5.4.5"
  }
}
