{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "outDir": "../../dist/out-tsc",
    "module": "commonjs",
    "types": ["cypress", "node"],
    "sourceMap": false,
    "baseUrl": "./",
    "paths": {
      "@hsi/*": ["dist/*"]
    }
  },
  "include": [
    "cypress.config.ts",
    "**/support/*.ts",
    "**/*.cy.ts",
    "**/*.cy.js",
    "**/*.d.ts"
  ]
}
