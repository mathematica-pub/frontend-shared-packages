{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "../node_modules",
    "target": "ES2019",
    "lib": ["es2020", "dom"],
    "types": ["cypress", "node", "cypress-real-events"],
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "typeRoots": ["../node_modules/@types", "support"],
    "noEmit": true
  },
  "include": ["./*/*.ts"]
}
